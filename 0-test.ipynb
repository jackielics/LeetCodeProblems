    "numRows = 5\n",
    "res = [[1]]\n",
    "for i in range(1, numRows):\n",
    "    new = []\n",
    "    for j in range(i+1):\n",
    "        if j in (0, i):\n",
    "            new.append(1)\n",
    "        else:\n",
    "            new.append(res[i-1][j-1] + res[i-1][j])\n",
    "    res.append(new)"
